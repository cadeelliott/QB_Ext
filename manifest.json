{
  "manifest_version": 3,
  "name": "JSON Fetcher Extension",
  "version": "1.0",
  "description": "Fetch JSON from a website",
  "permissions": ["activeTab", "http://*/*", "https://*/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
  "background": {
  "scripts": ["background.js"],
  "persistent": false
  },

}
